{
  "name": "babylon-js-windmill",
  "version": "1.0.0",
  "description": "Babylon.js windmill viewer",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon --watch src/server.ts --exec ts-node src/server.ts\"",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "babylonjs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@babylonjs/core": "^8.3.1",
    "@babylonjs/inspector": "^8.3.1",
    "@babylonjs/loaders": "^8.3.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.2"
  }
}
